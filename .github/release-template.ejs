var listCommits = function(list, key){

list.forEach(function (ct) {

    var type = ct.type;
    var scope = ct.scope;
    var title = '';
    var commit = ct.commit;

    if(type){
        if(key != 'other'){
            title = (scope? '__'+scope+'__: ':'') + ct.title;
        }else{
            title = '__' + type + (scope? '('+scope+')':'') + '__ : ' + ct.title;
        }
    }else{
        title = commit.title;
    }
